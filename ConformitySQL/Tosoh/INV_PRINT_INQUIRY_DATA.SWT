          'N',--ENABLED
          'Y',--CONFIGURABLE
          'Serial',--PROMPT
          'ESI_INVENTORY',--LOOKUP TABLE
          'SERIAL_NO',--FIELD NAME
          '',--DEFAULT VALUE
          '',--MAX LENGTH
          'N',--READ_ONLY
          'Y',--UPPERCASE
          'N',--HAS UOM
          '',--LOV OVERRIDE QUERY
          'Serial',--LOV PROMPT
          'N',--LOV ON ENTRY
          'N',--LOV READ ONLY
          SYSDATE);
   
   hierarchy_    := hierarchy_ + 1;
   
   --LOT_BATCH_NO
  INSERT INTO esi_sw_fields_tab
  VALUES (transaction_,
          hierarchy_,
          'LOT_BATCH_NO',--FIELD_ID
          'Y',--ENABLED
          'Y',--CONFIGURABLE
          'Lot',--PROMPT
          'ESI_INVENTORY',--LOOKUP TABLE
          'LOT_BATCH_NO',--FIELD NAME
          '',--DEFAULT VALUE
          '',--MAX LENGTH
          'N',--READ_ONLY
          'Y',--UPPERCASE
          'N',--HAS UOM
          '',--LOV OVERRIDE QUERY
          'Lot',--LOV PROMPT
          'N',--LOV ON ENTRY
          'N',--LOV READ ONLY
          SYSDATE);
   
   hierarchy_    := hierarchy_ + 1;

   --ENG_CHG_LEVEL
  INSERT INTO esi_sw_fields_tab
  VALUES (transaction_,
          hierarchy_,
          'ENG_CHG_LEVEL',--FIELD_ID
          'N',--ENABLED
          'Y',--CONFIGURABLE
          'EC',--PROMPT
          'ESI_INVENTORY',--LOOKUP TABLE
          'ENG_CHG_LEVEL',--FIELD NAME
          '',--DEFAULT VALUE
          '',--MAX LENGTH
          'N',--READ_ONLY
          'Y',--UPPERCASE
          'N',--HAS UOM
          '',--LOV OVERRIDE QUERY
          'EC',--LOV PROMPT
          'N',--LOV ON ENTRY
          'N',--LOV READ ONLY
          SYSDATE);
   
   hierarchy_    := hierarchy_ + 1;
   
   --WAIV_DEV_REJ_NO
  INSERT INTO esi_sw_fields_tab
  VALUES (transaction_,
          hierarchy_,
          'WAIV_DEV_REJ_NO',--FIELD_ID
          'N',--ENABLED
          'Y',--CONFIGURABLE
          'WDR',--PROMPT
          'ESI_INVENTORY',--LOOKUP TABLE
          'WAIV_DEV_REJ_NO',--FIELD NAME
          '',--DEFAULT VALUE
          '',--MAX LENGTH
          'N',--READ_ONLY
          'Y',--UPPERCASE
          'N',--HAS UOM
          '',--LOV OVERRIDE QUERY
          'WDR',--LOV PROMPT
          'N',--LOV ON ENTRY
          'N',--LOV READ ONLY
          SYSDATE);
   
   hierarchy_    := hierarchy_ + 1;
    
   --CONFIGURATION_ID
  INSERT INTO esi_sw_fields_tab
  VALUES (transaction_,
          hierarchy_,
          'CONFIGURATION_ID',--FIELD_ID
          'N',--ENABLED
          'Y',--CONFIGURABLE
          'Configuration',--PROMPT
          'ESI_INVENTORY',--LOOKUP TABLE
          'CONFIGURATION_ID',--FIELD NAME
          '',--DEFAULT VALUE
          '',--MAX LENGTH
          'N',--READ_ONLY
          'Y',--UPPERCASE
          'N',--HAS UOM
          '',--LOV OVERRIDE QUERY
          'Configuration',--LOV PROMPT
          'N',--LOV ON ENTRY
          'N',--LOV READ ONLY
          SYSDATE);
   
   hierarchy_    := hierarchy_ + 1;
     
 --EXPIRATION_DATE
  INSERT INTO esi_sw_fields_tab
  VALUES (transaction_,
          hierarchy_,
          'EXPIRATION_DATE',--FIELD_ID
          'N',--ENABLED
          'Y',--CONFIGURABLE
          'Expiration',--PROMPT
          'ESI_INVENTORY',--LOOKUP TABLE
          'TO_DATE(EXPIRATION_DATE, ''MM/DD/YYYY'')',--FIELD NAME
          '',--DEFAULT VALUE
          '',--MAX LENGTH
          'N',--READ_ONLY
          'Y',--UPPERCASE
          'N',--HAS UOM
          '',--LOV OVERRIDE QUERY
          'Expiration',--LOV PROMPT
          'N',--LOV ON ENTRY
          'N',--LOV READ ONLY
          SYSDATE);
   
   hierarchy_    := hierarchy_ + 1; 
   
    --QTY_ONHAND
     INSERT INTO esi_sw_fields_tab
     VALUES (transaction_,
             hierarchy_,
             'QTY_ONHAND',--FIELD_ID
             'Y',--ENABLED
             'Y',--CONFIGURABLE
             'Qty On Hand',--PROMPT
             'ESI_INVENTORY',--LOOKUP TABLE
             'QTY_ONHAND',--FIELD NAME
             '',--DEFAULT VALUE
             '',--MAX LENGTH
             'Y',--READ_ONLY
             'Y',--UPPERCASE
             'Y',--HAS UOM
             '',--LOV OVERRIDE QUERY
             'Qty On Hand',--LOV PROMPT
             'N',--LOV ON ENTRY
             'N',--LOV READ ONLY
             SYSDATE);
      
   hierarchy_    := hierarchy_ + 1;

     --QTY_RESERVED
     INSERT INTO esi_sw_fields_tab
     VALUES (transaction_,
             hierarchy_,
             'QTY_RESERVED',--FIELD_ID
             'N',--ENABLED
             'Y',--CONFIGURABLE
             'Qty Reserved',--PROMPT
             'ESI_INVENTORY',--LOOKUP TABLE
             'QTY_RESERVED',--FIELD NAME
             '',--DEFAULT VALUE
             '',--MAX LENGTH
