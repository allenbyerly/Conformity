             'Qty Reserved',--LOV PROMPT
             'N',--LOV ON ENTRY
             'N',--LOV READ ONLY
             SYSDATE);
      
   hierarchy_    := hierarchy_ + 1;

   --PRINT
     INSERT INTO esi_sw_fields_tab
     VALUES (transaction_,
             hierarchy_,
             'PRINT',--FIELD_ID
             'Y',--ENABLED
             'N',--CONFIGURABLE
             'Print (Y/N)',--PROMPT
             '',--LOOKUP TABLE
             'PRINT',--FIELD NAME
             'N',--DEFAULT VALUE
             '1',--MAX LENGTH
             'N',--READ_ONLY
             'Y',--UPPERCASE
             'N',--HAS UOM
             '',--LOV OVERRIDE QUERY
             'Print (Y/N)',--LOV PROMPT
             'N',--LOV ON ENTRY
             'N',--LOV READ ONLY
             SYSDATE);
      
   hierarchy_    := hierarchy_ + 1;
  
   --PRINT_QTY
  INSERT INTO esi_sw_fields_tab
  VALUES (transaction_,
          hierarchy_,
          'PRINT_QTY',--FIELD_ID
          'Y',--ENABLED
          'N',--CONFIGURABLE
          'Print Qty',--PROMPT
